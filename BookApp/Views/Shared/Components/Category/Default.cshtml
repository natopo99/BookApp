
@{
}

@model IEnumerable<string>

    <div class="d-grid gap-4">
            <a asp-controller="Home" asp-action="Index" asp-route-category="" asp-route-pageNum="1"
                class="btn btn-outline-danger">Home</a>
            <br />
            @foreach (string c in Model)
            {
                <a asp-controller="Home" asp-action="Index" asp-route-category="@c" asp-route-pageNum="1"
                    class="btn SelectedCategory  btn-primary btn-outline-danger">@c</a>
                    <br />
            }
    </div>



